{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: g++.exe build",
			"command": "F:\\CDev\\MinGW\\bin\\g++.exe",
			"args": [
				"-std=c++11",
				"-march=native",
				"-Ofast",
				"-g",
				"MakkiNeurons.cpp",
				"-o",
				"${fileDirname}\\Main.exe"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}